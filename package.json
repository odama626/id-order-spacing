{
  "name": "@sparkstone/id-order-spacing",
  "version": "1.0.1",
  "description": "A lightweight utility library for calculating minimal spacing changes when reordering items in a sorted array.",
  "type": "module",
  "source": "src/main.ts",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.modern.js",
      "types": "./dist/main.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "microbundle src/main.ts --generateTypes",
    "dev": "microbundle watch src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": ["dist", "README.md"],
  "keywords": [
    "order",
    "spacing",
    "minimal",
    "utility",
    "database",
    "typescript",
    "reordering"
  ],
  "author": "odama626",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odama626/id-order-spacing.git"
  },
  "bugs": {
    "url": "https://github.com/odama626/id-order-spacing/issues"
  },
  "homepage": "https://github.com/odama626/id-order-spacing#readme",
  "packageManager": "pnpm@10.4.1",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^5.7.3"
  }
}
